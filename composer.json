{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/admin_toolbar": "^1.23",
        "drupal/avatars": "^1.0",
        "drupal/bg_image_formatter": "^1.1",
        "drupal/bootstrap": "^3.11",
        "drupal/colorbox": "^1.4",
        "drupal/console": "^1.0.2",
        "drupal/core": "~8.5.3",
        "drupal/entity_reference_revisions": "^1.4",
        "drupal/field_slideshow": "2.x-dev",
        "drupal/filefield_paths": "^1.0",
        "drupal/geofield": "^1.0",
        "drupal/imagefield_slideshow": "^1.6",
        "drupal/juicebox": "^2.0",
        "drupal/libraries": "^3.0",
        "drupal/page_load_progress": "^1.2",
        "drupal/pathauto": "^1.3",
        "drupal/redirect": "^1.2",
        "drupal/slick_views": "^1.0",
        "drupal/social_media": "^1.3",
        "drupal/styled_google_map": "^1.2",
        "drupal/token": "^1.1",
        "drupal/video_embed_field": "^2.0",
        "drupal/views_slideshow": "^4.6",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        }
    },
    "patches": {
        "drupal/paragraphs": {
            "#2764681: Allow paragraph type deletion if content exists": "https://www.drupal.org/files/issues/2764681-allow-paragraphs-type-deletion-if-content-exists-55.patch"
        },
        "drupal/core": {
            "#2137815: Invalid translation language": "https://www.drupal.org/files/issues/2018-04-03/drupal_2137815_43.patch"
        }
    }
}
